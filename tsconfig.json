{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false,
    "paths": {
      "src-electron/*": ["src-electron/*"],
      "@root/*": ["./*"]
